   "source": [
    "import time\n",
    "import webbrowser\n",
    "\n",
    "breaks = 2\n",
    "n = int(input(\"Enter the number of hours in between two successive breaks: \"))\n",
    "\n",
    "for i in range(breaks):\n",
    "    time.sleep(n*60*60)\n",
    "    webbrowser.open('https://www.youtube.com/',new=2)"
   ]
  }
